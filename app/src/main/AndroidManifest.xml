<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.fran.electricapp" >

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <!--Permisos para los mapas-->
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="com.google.android.providers.gsf.permission.READ_GSERVICES" />
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme"
        android:name=".ParseApplication" >

        <meta-data
            android:name="com.google.android.gms.version"
            android:value="@integer/google_play_services_version" />
        <meta-data
            android:name="com.google.android.maps.v2.API_KEY"
            android:value="AIzaSyDX3VlRT5RpU4PXafzph9kgJx77woGBo7s" />
        <!--MAC_Debug:
        SHA1: 84:75:F1:E3:3E:23:62:BB:63:04:CD:C5:CA:62:E9:1D:87:39:0B:D9-->
        <!--UNISYS_Debug: AIzaSyDX3VlRT5RpU4PXafzph9kgJx77woGBo7s
        SHA1: 883:23:C9:2A:C0:47:9A:97:0F:C0:DA:C7:E5:A1:02:02:31:41:F5:F6;com.fran.electricapp-->

        <!--La actividad que se ejecuta al inicio de la aplicacion tiene la categoria LAUNCHER-->
        <activity android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name=".AppActivity"
            android:label="@string/app_name"
            android:configChanges="screenSize|orientation|keyboard"/>

    </application>

</manifest>
